{
  "name": "@entro314labs/entro-nextjs",
  "version": "1.0.0",
  "description": "Official Next.js integration for Entrolytics analytics",
  "author": "Entrolytics <hey@entrolytics.click>",
  "license": "MIT",
  "homepage": "https://entrolytics.click",
  "repository": {
    "type": "git",
    "url": "https://github.com/entro314-labs/entro-nextjs.git"
  },
  "keywords": [
    "analytics",
    "nextjs",
    "react",
    "tracking",
    "privacy",
    "entrolytics",
    "web-analytics"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js",
      "require": "./dist/plugin/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "biome check --write .",
    "lint:check": "biome check .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "next": "^16.0.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20"
  }
}
